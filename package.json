{
    "name": "site-svelte-app",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "pnpm dev:dev",
        "dev:dev": "vite --mode development build -w --sourcemap true",
        "dev:serve": "vite",
        "gulp": "gulp -f gulpfile.cjs",
        "gulp:dev": "gulp -f gulpfile.cjs watch",
        "start": "sirv public --single",
        "prettier": "npx prettier --write src",
        "ci": "npx svelte-check",
        "help": "echo \"dev: tmux => pnpm dev | pnpm gulp:dev; \nbuild: pnpm gulp => pnpm build\"",
        "postinstall": "pnpm run build",
        "install:win": "pnpm install --config.https-proxy=http://127.0.0.1:7890 --config.proxy=http://127.0.0.1:7890"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.5",
        "@sveltejs/vite-plugin-svelte": "^3.0.1",
        "@tsconfig/svelte": "^5.0.2",
        "autoprefixer": "^10.4.16",
        "cssnano": "^6.0.3",
        "gulp": "^4.0.2",
        "gulp-postcss": "^9.0.1",
        "gulp-purgecss": "^4.1.3",
        "gulp-sass": "^5.1.0",
        "gulp-sourcemaps": "^3.0.0",
        "postcss": "^8.4.33",
        "postcss-cli": "^9.1.0",
        "prettier": "^3.1.1",
        "prettier-plugin-svelte": "^3.1.2",
        "rollup": "^4.9.4",
        "rollup-plugin-livereload": "^2.0.5",
        "rollup-plugin-svelte": "^7.1.6",
        "sass": "^1.69.7",
        "sirv-cli": "^1.0.14",
        "svelte": "^4.2.8",
        "svelte-check": "^3.6.2",
        "svelte-preprocess": "^5.1.3",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3",
        "vite": "^5.0.11",
        "vite-tsconfig-paths": "^4.3.1"
    },
    "dependencies": {
        "@shopify/draggable": "1.0.0-beta.8",
        "animate.css": "^4.1.1",
        "bootstrap-icons": "^1.11.3",
        "highlight.js": "^11.9.0",
        "howler": "^2.2.4",
        "marked": "^11.1.1",
        "page.js": "^4.13.3",
        "simple-line-icons": "^2.5.5",
        "svelte-terminal": "github:candywater/svelte-terminal#0.0.2",
        "tailwindcss": "^3.4.1"
    },
    "packageManager": "pnpm@8.15.1"
}